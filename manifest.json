{
    "manifest_version": 2,
    "name": "BioMio",
    "description": "This extension will allow you to encrypt/decrypt your emails",
    "version": "1.0",
    "permissions": ["activeTab", "notifications", "storage"],
    "optional_permissions": [],
    "background": {
        "scripts": ["e2e_library/end-to-end.compiled.js", "end_to_end_client.js"/*"state-machine.js", "socket_requests.js",
            "rsa_sign_library/ext/base64.js",
            "rsa_sign_library/ext/jsbn.js",
            "rsa_sign_library/ext/jsbn2.js",
            "rsa_sign_library/ext/rsa.js",
            "rsa_sign_library/ext/rsa2.js",
            "rsa_sign_library/ext/cryptojs-312-core-fix.js",
            "rsa_sign_library/rsasign-1.2.js",
            "rsa_sign_library/rsapem-1.1.js",
            "rsa_sign_library/crypto-1.1.js",
            "rsa_sign_library/asn1hex-1.1.js",
            "rsa_sign_library/x509-1.1.js",
            "rsa_sign_library/jsrsasign-latest-all-min.js",
            "background.js"*/]
    },
    "browser_action": {
        "default_title": "BioMio",
        "default_icon": "images/smallLogo.png"
    },
    "icons": {
        "16": "images/smallLogo.png",
        "48": "images/smallLogo.png",
        "128": "images/smallLogo.png"
    },
    "web_accessible_resources": ["additional_html.html", "*.js"],
    "default_locale": "en",
    "key": "",
    "content_scripts": [
        {
            "matches": ["*://mail.google.com/*"],
            "js": ["jquery-1.11.1.js", "e2e_library/end-to-end.compiled.js", "contentscript.js"],
            "run_at": "document_end",
            "css": ["bio_styles.css"]
        }
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
